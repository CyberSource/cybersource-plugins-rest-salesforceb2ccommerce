<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="Cybersource_VisaSRCEnabled">
                <display-name xml:lang="x-default">Enable Visa SRC</display-name>
                <description xml:lang="x-default">Enable/Disable Enable Visa SRC on checkout page</description>
                <type>boolean</type>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="Cybersource_VisaSRCKey">
                <display-name xml:lang="x-default">Visa SRC Key</display-name>
                <description xml:lang="x-default">Visa SRC Key Id obtained through EBC Digital payments</description>
                <type>string</type>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="Cybersource_VisaSRCProduction">
                <display-name xml:lang="x-default">True for production</display-name>
                <description xml:lang="x-default">Set to Yes for Production</description>
                <type>boolean</type>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Cybersource_VisaSRC">
                <display-name xml:lang="x-default"></display-name>
                <attribute attribute-id="Cybersource_VisaSRCEnabled"/>
                <attribute attribute-id="Cybersource_VisaSRCKey"/>
                <attribute attribute-id="Cybersource_VisaSRCProduction"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>